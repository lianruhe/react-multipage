{
  "presets": [
    "es2015",
    "stage-2",
    ["env", {
      "modules": false,
      "targets": {
        "ie9": true,
      },
      "uglify": true,
    }],
    "react"
  ],
  "plugins": [
    "add-module-exports",
    "syntax-dynamic-import",
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    "transform-runtime",
    "react-hot-loader/babel",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
   ],
   "comments": false
}
