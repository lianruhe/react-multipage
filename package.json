    "babel-preset-stage-0": "^6.5.0-1",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^4.0.0",
    "cross-spawn": "^5.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.9.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.0.1",
