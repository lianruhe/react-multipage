    "query-string": "^4.3.4",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "string-template": "^1.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "antd": "^2.9.3",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.0",
